<html>
	<tr>
		<td class="top-action">
			<img src="https://img.icons8.com/small/1600/filled-folder.png">
			<a href='/dir/{path}'>.</a>
		</td>
		<td>
		</td>
		<td>
		</td>
		<td>
		</td>
	</tr>
	{@eq key=squery value=true type="boolean"}
		{@ne key=previous value="" type="string"}
			<tr>
				<td class="top-action">
					<img src="https://img.icons8.com/small/1600/filled-folder.png">
					<a href='/dir/{previous}'>..</a>
				</td>
				<td>
				</td>
				<td>
				</td>
				<td>
				</td>
			</tr>
		{/ne}
	{/eq}
	{# list}
	<tr>
		{@eq key=isDir value=false type="boolean"}
			<td>
				<img src="https://cdn3.iconfinder.com/data/icons/brands-applications/512/File-512.png">
				<a href='/file/preview/{path}' target="_blank">{name}</a>
			</td>
			<td>
				{size}
			</td>
			<td>
				<!-- {timeCreated}<br> -->
				{dateCreated}
			</td>
			<!-- <td>
				{tags}
			</td> -->
			<td>
				<ul>
					<li><button class="file-action"></button></li>
					<li><button class="file-action"></button></li>
					<li><button class="file-action"></button></li>
					<li><button class="file-action"></button></li>
				</ul>
			</td>
		{:else}
			<td>
				<img src="https://img.icons8.com/small/1600/filled-folder.png">
				<a href='/dir/{path}'>{name}</a>
			</td>
			<td>
			</td>
			<td>
				<!-- {timeCreated}<br> -->
				{dateCreated}
			</td>
			<!-- <td>
				{tags}
			</td> -->
			<td>
				<ul>
					<li><button class="file-action"></button></li>
					<li><button class="file-action"></button></li>
					<li><button class="file-action"></button></li>
					<li><button class="file-action"></button></li>
				</ul>
			</td>
		{/eq}
	</tr>
	{/ list}
</html>
